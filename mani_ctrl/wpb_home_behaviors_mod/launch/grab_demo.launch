<launch>

    <!-- Run grab server -->
    <include file="$(find wpb_home_behaviors_mod)/launch/grab_app.launch" />

    <!-- Run grab monitor -->
    <include file="$(find wpb_home_behaviors_mod)/launch/grab_monitor.launch" />

    <!-- Run wpb_home_grab_client node -->
    <node name="wpb_home_mani_ctrl" pkg="wpb_home_behaviors_mod" type="wpb_home_grab_client"  output="screen"/>

</launch>
